{
  "name": "time-management",
  "version": "1.0.0",
  "description": "Amir template",
  "main": "dist",
  "scripts": {
    "build": "npx tsc",
    "start:DEV": "ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "seeder": "ts-node -r tsconfig-paths/register src/seed/seeder.ts",
    "start": "node ./dist/index.js",
    "start:prod": "concurrently -c \"auto\" \"npm:build && node ./dist/index.js\"",
    "lint": "eslint . --ext .ts",
    "development": "nodemon --exec \"npm run lint & npm run start:DEV\"",
    "start:dev": "concurrently -c \"auto\" \"npm:development\""
  },
  "keywords": [],
  "author": "amir ch",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.6.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.14",
    "@types/webidl-conversions": "^7.0.3",
    "@types/whatwg-url": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma-kysely": "^1.7.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
